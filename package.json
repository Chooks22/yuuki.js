{
  "name": "yuuki.js",
  "version": "0.1.0",
  "description": "Next generation discord bot framework",
  "type": "module",
  "license": "MIT",
  "files": [
    "./dist"
  ],
  "bin": {
    "yuuki": "dist/bin.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup-node src --clean --target es2022 --format esm --dts",
